{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  },
  "source": {
    "include": ["lib", "errors"],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(node_modules/|docs)"
  },
  "plugins": ["plugins/markdown", "node_modules/better-docs/category"],
  "opts": {
    "template": "./node_modules/better-docs/",
    "encoding": "utf8",
    "destination": "docs/",
    "recurse": true,
    "verbose": true
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "better-docs": {
      "title": "Percipio Axios Client",
      "hideGenerator": true,
      "navLinks": [
        {
          "label": "Github",
          "href": "https://github.com//martinholden-skillsoft/percipio-axios"
        }
      ]
    }
  }
}
